{
    "compilerOptions": {
        "target": "ES2017",
        "module": "ES6",
        "moduleResolution": "node",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "suppressImplicitAnyIndexErrors": true,
        "removeComments": false,
        "keyofStringsOnly": true,
        "outDir": "dist"
    },
    "exclude": [
        "node_modules"
    ]
}
